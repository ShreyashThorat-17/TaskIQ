{
    "version": 2,
    "builds": [
        {
            "src": "dist/task-iq/browser/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/dist/task-iq/browser/$1"
        }
    ],
    "git": {
        "deploymentEnabled": false
    }
} 